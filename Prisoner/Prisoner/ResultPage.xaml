<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Prisoner.ResultPage">
    <ContentPage.Content>
        <StackLayout Margin="5,5,5,5">
            <Label Text="Прописка окончена..." HorizontalOptions="CenterAndExpand" FontSize="20" />
            <Label x:Name="resultLabel" Text="Чики брики в дамки!"/>
            <Button Text="Узнать результат" Clicked="ButtonClick"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>